(function (window, $) {

  window.FACTORS = <%= BudgetPlanner::Total::FACTORS.to_json.html_safe %>;

  window.FrequencySelector = function FrequencySelector(element) {
    var self, factors;
    self = this;
    factors = window.FACTORS;

    this.factor = function factor() {
      return factors[element.val()];
    };

    element.on('change', function () { $(self).trigger('change'); });
  };

}(this, jQuery));
